{
  "title": "Graph",
  "type": "object",
  "properties":
  {
    "Node":
    {
      "type": "object",
      "properties":
      {
        "id":
        {
          "description": "Node Id",
          "type": "integer"
        },
        "xPosition":
        {
          "description": "x coordinate",
          "type": "integer"
        },
        "yPosition":
        {
          "description": "y coordinate",
          "type": "integer"
        },
        "visited":
        {
          "description": "Verify if this node has been visited",
          "type": "boolean"
        },
        "neighbors":
        {
          "description": "Neighbors to this node",
          "type": "array",
          "items":
          {
            "type": "node"
          }
        },
        "parent":
        {
          "description": "Parent Node",
          "type": "node"
        }
      },
      "required": ["id"]
    },
    "Edge":
    {
      "type": "object",
      "properties":
      {
        "source":
        {
          "description": "Source node",
          "type": "Node"
        },
        "destination":
        {
          "description": "Destination node",
          "type": "Node"
        },
        "weight":
        {
          "description": "cost of taking edge between Nodes",
          "type": "number"
        },
        "isSettled":
        {
          "description": "Check if this edge has been settled. Used in Dijkstra's algorithm",
          "type": "boolean"
        }
      },
      "required": ["source", "destination"]
    },
    "NodeList":
    {
      "description": "List containing all Nodes in Graph",
      "type": "array"
    },
    "EdgeList":
    {
      "description": "List containing all Edges in Graph",
      "type": "array"
    }
  }
}